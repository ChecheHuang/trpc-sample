generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model todos {
  id         Int      @id @default(autoincrement())
  content    String   @unique
  done       Boolean  @default(false)
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
